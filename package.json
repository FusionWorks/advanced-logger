{
  "name": "@fusionworks/advanced-logger",
  "version": "0.0.1",
  "private": false,
  "description": "Advanced console logger which replace console.log adding a lot more configurability.",
  "main": "dist/index.js",
  "scripts": {
    "rollup": "rollup",
    "build": "rollup -c",
    "postbuild": "node lib/copy_package",
    "watch": "rollup -cw",
    "patch": "npm version patch",
    "prerelease": "npm whoami && npm run patch && npm run build",
    "release": "npm publish dist --access public --dry-run"
  },
  "keywords": [
    "npm",
    "tsc",
    "typescript",
    "log",
    "console",
    "logger",
    "advanced"
  ],
  "author": "GrigoreMe",
  "license": "ISC",
  "devDependencies": {
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "tslib": "^1.9.3",
    "typescript": "^3.3.4000",
    "@types/node": "^11.12.0"
  },
  "dependencies": {}
}
